{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.cjs",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/login",
      "destination": "/api/login.cjs"
    },
    {
      "source": "/api/register",
      "destination": "/api/register.cjs"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}